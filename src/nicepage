body {
  display: flex;
  flex-direction: column;
  
}

button {
  padding: 12px 24px; /* Increased padding for a bigger button */
  margin: 10px; /* Adjusted margin for better spacing */
  width: 150px; /* Set a wider width for a more prominent button */
  font-size: 1.2em; /* Increased font size for better readability */
  color: #333;
  background-color: white;
  border: 2px solid #333;
  border-radius: 5px;
  cursor: pointer;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
  transition: background-color 0.3s ease, color 0.3s ease, border-color 0.3s ease;
}

button:hover,
button:focus {
  background-color: #333;
  color: white;
  border-color: #333;
}

button:disabled {
  background-color: #e0e0e0;
  color: #666;
  border-color: #999;
  cursor: not-allowed;
} 

